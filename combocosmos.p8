texts = {}

function make_text(x,y,str,clr,life)
	local text = {}
	text.x = x
	text.y = y
	text.str = str
	text.clr = clr
	text.life = life
	text.dy = -0.25
	
	text.update = function(this)
		this.y += this.dy
		this.life -= 1
		if this.y < 0 or this.life < 0 then
			del(texts,this)
		end
	end
	
	text.draw = function(this)
		print(this.str,this.x,this.y,this.clr)
	end
	
	add(texts,text)
end
