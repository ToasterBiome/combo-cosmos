function make_player()
	local player = {}
	player.x = 64
	player.y = 64
	player.dx = 0
	player.dy = 0
	player.sprite = 3
	player.max_speed = 1
	player.particle_timer = 0
	player.box = {x1=2,x2=6,y1=2,y2=6}
	player.shield = false
	player.shield_anim = {36,37,38,39,40,40,40,40}
	player.flame_anim = {54,55,56,57}
	player.flaming = false
	player.update_sprite = function(this)
		this.sprite = rnd(6)+1
	end
	
	player:update_sprite()
	
	player.update = function(this)
		local updated = false
		if btn(0) then
			this.dx -= (4/60)
			updated = true
		end
		if btn(1) then
			this.dx += (4/60)
			updated = true
		end
		if btn(2) then
			this.dy -= (4/60)
			updated = true
		end
		if btn(3) then
			this.dy += (4/60)
			updated = true
		end
		if	btn(4) and this.flaming then
			this.flaming = false
			make_explosion_flame_boost(this.x,this.y)
			meteor_speed += 0.5
			intensity += 0.125
			sfx(18,1)
			updated = true
		end
		
		if not updated then
			this.dx *= 0.95
			this.dy *= 0.95
		end
		
		if this.x + this.dx > 128 - 10 then
			this.dx = -this.max_speed * 0.5
		end
		if this.x - this.dx < 4 then
			this.dx = this.max_speed * 0.5
		end
		if this.y + this.dy > 128 - 18 and not game_over then
			this.dy = -this.max_speed * 0.5
		end
		if this.y - this.dy < 4 then
			this.dy = this.max_speed * 0.5
		end
		
		
		if this.dx != 0 then
			this.dx = mid(0,this.dx,sgn(this.dx) * this.max_speed)
		
		end
		if this.dy != 0 then
			this.dy = mid(0,this.dy,sgn(this.dy) * this.max_speed)
		end
		this.particle_timer += 1
		if this.particle_timer > 4 and (this.dx != 0 or this.dy != 0) then
			this.particle_timer = 0
			new_particle(this.x,this.y,-this.dx,-this.dy,52,"blue",15)
		end
		
		--collisions
		for m in all(meteors) do
			if coll(this,m) then
				del(meteors,m)
				intensity = 0.0625
				if this.shield then
					make_explosion_shield(this.x,this.y)
					this.shield = false
					this:update_sprite()
					this.dx = -meteor_speed * 0.5
					sfx(16,1)
				elseif this.flaming then
					this.flaming = false
					this:update_sprite()
					sfx(16,1)
					for i=1,4 do
						make_debris(this.x,this.y,rnd(1)-0.5,rnd(1)-0.5)
					end
				else
					meteor_speed -= 0.5
					this:update_sprite()
					make_explosion(this.x,this.y)
					score = max(0,score - 50)
					make_text(this.x,this.y,"-50",8,60)
					if meteor_speed < 1 then
						--you lose
						game_over = true
						meteors = {}
						boosts = {}
						texts = {}
						debris_list = {}
						this.dy = -4
						this.dx = rnd(4)-2
						sfx(2,1)
						music(5)
						if score > hi_score then
							hi_score = score
						end
					else
						sfx(0,1)
					end
				end
			end
		end
		
		for b in all(boosts) do
			if coll(this,b) then
				if b.type == "boost" then
					make_explosion_boost(this.x,this.y)
					meteor_speed += 0.25
					sfx(1,1)
				elseif b.type == "shield" then
					make_explosion_shield(this.x,this.y)
					this.shield = true
					this.flaming = false
					sfx(15,1)
				elseif b.type == "flame" then
					make_explosion_flame(this.x,this.y)
					this.flaming = true
					this.shield = false
					sfx(15,1)
				end
				score += b.points
				make_text(this.x,this.y,"+" .. b.points,11,60)
				del(boosts,b)
				end
		end
		
		if game_over then
			this.dy += (4/60)
		end
		
		this.x += this.dx
		this.y += this.dy
		
	end
	
	player.draw = function(this)
		pal()
		spr(this.sprite,this.x,this.y)
		if this.shield then
			spr(getframe(this.shield_anim),this.x,this.y)
		end
		if this.flaming then
			spr(getframe(this.flame_anim),this.x,this.y)
		end
		
		--(this.x,this.y,1,8)
	end
	
	
	
	return player
end
