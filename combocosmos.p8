function _init()

	star_timer = 0
	make_colors()
	player = make_player()
	game_over = false
	intro = true
	music(0)
	score = 0
	hi_score = 0
	for i=0,20 do
		col = colors_string[ceil(rnd(4))]
		new_particle(rnd(128),rnd(128),-rnd(0.5) - 0.25,rnd(0.125)-0.0625,rnd(5) + 48,col)
	end
end

function _update60()
	for p in all(particles) do
		p:update()
	end
	star_timer += 1
	if star_timer > 12 then
		star_timer = 0
		col = colors_string[ceil(rnd(4))]
		new_particle(127,rnd(128),-rnd(0.5) - 0.25,rnd(0.125)-0.0625,rnd(5) + 48,col)
	end
	
	if intro then
		if btn(4) then
			intro = false
			sfx(14,2)
			make_explosion_combo(player.x,player.y)
			meteor_speed = 1
		end
		return
	end
	
	if game_over then
		player:update()
		if btn(4) then
			game_over = false
			player.x = 64
			player.y = 64
			player.dx = 0
			player.dy = 0
			meteor_speed = 1
			music(0)
			score = 0
			sfx(14,2)
			make_explosion_combo(player.x,player.y)
			end
		return
	end
	meteor_timer += meteor_speed
	if meteor_timer > 30 then
		meteor_timer = 0
		if(rnd() > 0.1) then
			make_meteor()
		else
			make_boost()
		end
	end
	for m in all(meteors) do
		m:update()
	end
	for b in all(boosts) do
		b:update()
	end
	for t in all(texts) do
		t:update()
	end
	player:update()
end

function _draw()
	cls()
	for p in all(particles) do
		p:draw()
	end
	for m in all(meteors) do
		m:draw()
	end
	for b in all(boosts) do
		b:draw()
	end
	for t in all(texts) do
		t:draw()
	end
	if not intro then
		player:draw()
	end
	
	rect(0,0,127,127,5)
	
	if not intro then
	rectfill(0,127-8,127,127,13)
	local speed_text = "speed:"
	for i=1,min(meteor_speed,2.75),0.25 do
		speed_text = speed_text .. "★"
	end
	print(speed_text,4,120,7)
	print(score,127 - (#tostring(score) * 4),120,7)
	end
	
	
	
	if game_over then
		nicetext("game over",48,64-8)
		nicetext("press z to restart",32,64)
		nicetext("high score: " .. hi_score,32,64+8)
		nicetext("score: " .. score,32,64+16)
	end
	
	if intro then
		draw_intro(0,0)
	end
end

function draw_intro(x,y)
	shit = {}
	shit["c"] = 7
	shit["o"] = 8
	shit["m"] = 9
	shit["b"] = 10
	shit["s"] = 11
	str = "combo cosmos"
	--print(#str,0,0)
	for i=0,#str do
		local c = sub(str,i,i)
		local s = shit[c]
		if(s) then
			pal(7,5)
			spr(s,(i - 1) * 8 + 8,9)
			pal()
			spr(s,(i - 1) * 8 + 8,8)
		end
	end
	nicetext("prismapunch games",9,18)

	nicetext("⬆️⬇️⬅️➡️ to move", 32, 128-24) 
	nicetext("z to start",44,128-16)
end

function nicetext(text,x,y)
	print(text,x,y+1,5)
	print(text,x,y,7)
end
