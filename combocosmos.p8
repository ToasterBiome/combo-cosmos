debris_list = {}

function make_debris(x,y,dx,dy)
	local debris = {}
	debris.x = x
	debris.y = y
	debris.dx = dx
	debris.dy = dy
	debris.box = {x1=2,x2=6,y1=2,y2=6}
	
	debris.update = function(this)
		this.dy += 0.0125
		this.x += this.dx
		this.y += this.dy
		if not in_bounds(this.x,this.y) then
			del(debris_list, this)
		end
		if t % 16 == 0 then
			new_particle(this.x,this.y,-dx * 0.25,-dy * 0.25,52,"red",30,nil)
		end
		for m in all(meteors) do
			if coll(this,m) then
				del(meteors,m)
				make_text(this.x,this.y,"+25",11,60)
				score += 25
				del(debris_list, this)
				sfx(17,1)
			end
		end
	end
	
	debris.draw = function(this)
		pal()
		spr(23,this.x,this.y)
	end
	
	add(debris_list,debris)
end
