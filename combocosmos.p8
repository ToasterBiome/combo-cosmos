meteors = {}
meteor_speed = 0
meteor_timer = 0

function make_meteor()
	local meteor = {}
	meteor.x = 128 + 4
	meteor.y = rnd(128-16)
	meteor.sprite = rnd(7) + 16
	meteor.box = {x1=2,x2=6,y1=2,y2=6}
	meteor.update = function(this)
		this.x -= meteor_speed
		if this.x < 0 then
			del(meteors,this)
		end
	end
	
	meteor.draw = function(this)
		pal()
		spr(this.sprite,this.x,this.y)
	end
	add(meteors,meteor)
end

boosts = {}

function make_boost()
	local boost = {}
	boost.x = 128 + 4
	boost.y = rnd(128-16)
	boost.box = {x1=0,x2=8,y1=0,y2=8}
	
	rand_val = rnd()
	
	if rand_val > 0.6 then
		boost.type = "boost"
		boost.sprite = 12
		boost.points = 100
		boost.color = "blue"
	elseif rand_val > 0.3 then
		boost.type = "shield"
		boost.sprite = 13
		boost.points = 50
		boost.color = "green"
	else
		boost.type = "flame"
		boost.sprite = 14
		boost.points = 75
		boost.color = "red"
	end
	
	boost.update = function(this)
		this.x -= meteor_speed
		if this.x < 0 then
			del(boosts,this)
		end
		if t % 8 == 0 then
			new_particle(this.x,this.y,rnd(0.25)-0.125,rnd(0.25)-0.125,50,this.color,120,"mid")
		end
	end
	boost.draw = function(this)
		pal()
		spr(this.sprite,this.x,this.y)
	end
	
	add(boosts,boost)
end
