particles = {}

function make_colors()
	colors = {}
	colors_string = {"red","yellow","green","blue"}
	colors["red"] = {8,2}
	colors["yellow"] = {10,9}
	colors["green"] = {11,3}
	colors["blue"] = {12,1}
	anim = {}
	anim_string = {"big","mid","sml","flm"}
	anim["big"] = {48,49}
	anim["mid"] = {50,51}
	anim["sml"] = {52}
	anim["flm"] = {58,59,60,61}
end

function new_particle(x,y,dx,dy,sprite,col,life,animation)
	local particle = {}
	particle.x = x
	particle.y = y
	particle.dx = dx
	particle.dy = dy
	particle.sprite = sprite
	particle.col = col
	particle.animation = animation
	particle.ot = animation == "flm"
	particle.otv = 0
	if(life == nil) then particle.life = 800 else particle.life = life end
	particle.draw = function(this)
		pal(12,colors[this.col][1])
		pal(1,colors[this.col][2])
		if this.animation then
			spr(getframe(anim[this.animation],this.otv),this.x,this.y)
		else
			spr(this.sprite,this.x,this.y)
		 end
	 end
	particle.update = function(this)
		this.x += dx
		this.y += dy
		if this.ot then
			this.otv += 1
		end
		if not in_bounds(this.x,this.y) then
			del(particles,this)
		end
		this.life -= 1
		if this.life < 0 then
			del(particles,this)
		end
	end
	add(particles,particle)
end

function make_explosion(x,y)
	for i=0,10 do
		local dx = rnd(4)-2
		local dy = rnd(4)-2
		new_particle(x,y,dx,dy,52,"blue",60)
	end
end

function make_explosion_boost(x,y)
	for i=0,4 do
		local dx = -rnd(1) - 1
		local dy = rnd(2)-1
		new_particle(x,y,dx,dy,53,"blue",60)
	end
end

function make_explosion_shield(x,y)
	for i=0,4 do
		local dx = rnd(1) - 1
		local dy = rnd(1) - 1
		new_particle(x,y,dx,dy,53,"green",60,"mid")
	end
end

function make_explosion_flame(x,y)
	for i=0,4 do
		local dx = rnd(1) - 1
		local dy = rnd(1) - 1
		new_particle(x,y,dx,dy,53,"red",60,"mid")
	end
end

function make_explosion_flame_boost(x,y)
	for i=0,8 do
		local dx = -rnd(2)
		local dy = rnd(1) - 0.5
		new_particle(x,y,dx,dy,58,"red",48,"flm")
	end
end

function make_explosion_combo(x,y)
	for i=0,10 do
		local dx = rnd(2)-1
		local dy = rnd(2)-1
		new_particle(x,y,dx,dy,52,"blue",60)
	end
end

function handle_shake()
	local offset_x = rnd(8)-4
	local offset_y = rnd(8)-4
	
	offset_x *= intensity
	offset_y *= intensity
	camera(offset_x,offset_y)
	intensity *= 0.97
	if intensity < 0.05 then
			intensity = 0
			camera(0,0)
	end
end




